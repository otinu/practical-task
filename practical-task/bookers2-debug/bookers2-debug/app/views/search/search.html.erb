<% if @model == "user" %>
<div class="row  justify-content-center">
  <h2>ユーザ 一覧</h2>
</div>
<div class="row  justify-content-center">
  <% @datas.each do |datas| %>
  <table class="table table-striped table-bordered">
    <thead>
        <tr>
            <th>ユーザー名</th>
            </tr>
       </thead>
    <tbody>
        <tr>
            <td>
              <%=  datas.name %>
            </td>
        </tr>
    </tbody>
    </table>
    <% end %>
</div>

<% elsif @model == "book"%>
<% @datas.each do |datas| %>
<div class="row  justify-content-center">
<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <th>タグ一覧</th>
            </tr>
       </thead>
    <tbody>
        <tr>
            <td>
                <%= datas.title %>
            </td>
        </tr>
    </tbody>
    </table>
</div>
<% end %>
<% else %>
<% end %>
